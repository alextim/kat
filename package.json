{
  "name": "kat",
  "private": true,
  "description": "\"Tarutino Steppe\" website",
  "version": "0.0.1",
  "author": "Oleksii Tymoshenko <buildmnt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:uk": "cross-env LOCALES=uk ONLY=/ gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --fix src/**/*.{js,jsx,md}",
    "stylelint": "stylelint --syntax css-in-js src/**/*.{js,jsx,ts,tsx,css,scss}",
    "format": "prettier-eslint --write \"**/*.{js,jsx,json,md}\"",
    "translations:json2yaml": "node ./scripts/translations-json-to-yaml.js",
    "image:icon-svg-2-png": "node ./scripts/icon-svg-to-png.js",
    "image:favicon": "node ./scripts/icon-png-to-favicon-ico.js",
    "clean": "gatsby clean",
    "clean:build": "rimraf public/**/*.map",
    "deploy": "firebase deploy --only hosting",
    "deploy:clean": "yarn run clean:build && firebase deploy --only hosting",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@theme-ui/presets": "^0.3.0",
    "disable-scroll": "^0.5.1",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.24.11",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.22",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.21",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-remark-responsive-iframe": "^2.4.11",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-remark": "^2.8.27",
    "gatsby-transformer-sharp": "^2.5.11",
    "gatsby-transformer-yaml": "^2.4.10",
    "lazysizes": "^5.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "react-image-lightbox": "^5.1.1",
    "react-obfuscate": "^3.6.7",
    "react-scripts": "^3.4.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.4.1",
    "audit-ci": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "preval.macro": "^5.0.0",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "repository": "https://github.com/alextim/kat",
  "bugs": {
    "url": "https://github.com/alextim/kat"
  }
}
